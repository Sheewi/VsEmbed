{
  "name": "vsembed-ai-devtool",
  "version": "0.1.0",
  "description": "Portable, embeddable AI-powered development environment with VS Code engine",
  "main": ".webpack/main/index.js",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js",
    "build:core": "webpack --config webpack.core.config.js",
    "build:ai-components": "webpack --config webpack.ai.config.js",
    "build:extensions": "webpack --config webpack.extensions.config.js",
    "build-release": "npm run build",
    "dev": "npm run dev:main && npm run dev:renderer",
    "dev:main": "webpack --config webpack.main.config.js --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js",
    "start": "electron .",
    "start:mock-ai": "node tests/mocks/mock-ai-server.js",
    "package": "npm run build && electron-builder",
    "package:full": "npm run build:core && npm run build:ai-components && electron-builder",
    "package:ai-models": "node scripts/package-ai-models.js",
    "make": "npm run build && electron-builder --publish=never",
    "make:all-platforms": "electron-builder --linux --windows --mac --publish=never",
    "create:installer": "node scripts/create-installer.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:golden": "jest --testPathPattern=golden --updateSnapshot",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:e2e": "jest --testPathPattern=e2e --runInBand",
    "test:e2e:ai": "jest --testPathPattern=e2e-ai --runInBand",
    "test:performance": "jest --testPathPattern=performance",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:check": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "security:check": "npm audit --audit-level=moderate",
    "security:audit": "npm audit --audit-level=high",
    "security:scan": "node scripts/security-scan.js",
    "export-workspace": "node scripts/export-workspace.js",
    "import-workspace": "node scripts/import-workspace.js"
  },
  "keywords": [
    "ai",
    "development",
    "vscode",
    "electron",
    "portable",
    "sandbox"
  ],
  "author": "Sheewi",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "css-loader": "^6.7.0",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "eslint": "^8.37.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "reflect-metadata": "^0.1.13",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.0.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@vscode/extension-telemetry": "^0.6.2",
    "axios": "^1.3.0",
    "crypto-js": "^4.1.0",
    "dockerode": "^4.0.7",
    "electron-store": "^8.1.0",
    "inversify": "^6.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.539.0",
    "minimatch": "^10.0.3",
    "monaco-editor": "^0.45.0",
    "monaco-languageclient": "^4.0.3",
    "rclone": "^1.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "uuid": "^9.0.0",
    "vscode-languageclient": "^9.0.0",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.8",
    "ws": "^8.13.0",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0"
  },
  "build": {
    "appId": "com.sheewi.vsembed",
    "productName": "VSEmbed AI DevTool",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
