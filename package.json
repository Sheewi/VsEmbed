{
  "name": "vsembed-ai-devtool",
  "version": "0.1.0",
  "description": "Portable, embeddable AI-powered development environment with VS Code engine",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js",
    "dev": "npm run dev:main && npm run dev:renderer",
    "dev:main": "webpack --config webpack.main.config.js --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js",
    "start": "electron .",
    "package": "npm run build && electron-builder",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "export-workspace": "node scripts/export-workspace.js",
    "import-workspace": "node scripts/import-workspace.js"
  },
  "keywords": [
    "ai",
    "development",
    "vscode",
    "electron",
    "portable",
    "sandbox"
  ],
  "author": "Sheewi",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "css-loader": "^6.7.0",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.37.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.13.0"
  },
  "dependencies": {
    "@vscode/vscode-languagedetection": "^1.0.0",
    "@vscode/extension-telemetry": "^0.6.2",
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.8",
    "axios": "^1.3.0",
    "crypto-js": "^4.1.0",
    "dockerode": "^3.3.0",
    "electron-store": "^8.1.0",
    "monaco-editor": "^0.45.0",
    "monaco-languageclient": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rclone": "^2.0.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0"
  },
  "build": {
    "appId": "com.sheewi.vsembed",
    "productName": "VSEmbed AI DevTool",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
